import discord
from discord.ext import commands
bot = commands.Bot(command_prefix="*")

@bot.event
async def on_ready():
    print("Bot lancer !!!")
    print("Youpi ! ")
    await bot.change_presence(status=discord.Status.online, activity=discord.Game('*help'))

@bot.command()
async def salut(ctx):
    await ctx.send("Salut !")


@bot.command()
async def cv(ctx):
    await ctx.send("oui et toi")





bot.run('NzU1Nzk3NzY4MzMyNjQwMjU3.X2Ihhg.YZEl2preuNBdDGEtdC6nvbLhjFU')
